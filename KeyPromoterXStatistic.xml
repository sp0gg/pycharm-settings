<application>
  <component name="KeyPromoterXStatistic">
    <option name="statistics">
      <Statistic Action="Change Signature...">
        <Item>
          <IdeaActionID>ChangeSignature</IdeaActionID>
          <ShortCut>'Ctrl+F6'</ShortCut>
          <Description>Change Signature...</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Generate...">
        <Item>
          <IdeaActionID>Generate</IdeaActionID>
          <ShortCut>'Alt+Insert'</ShortCut>
          <Description>Generate...</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
    </option>
    <option name="suppressed">
      <Statistic Action="Back">
        <Item>
          <IdeaActionID>Back</IdeaActionID>
          <ShortCut>'Ctrl+Alt+Left' or 'Button4 Click'</ShortCut>
          <Description>Back</Description>
          <Count>3</Count>
        </Item>
      </Statistic>
      <Statistic Action="Copy">
        <Item>
          <IdeaActionID>$Copy</IdeaActionID>
          <ShortCut>'Ctrl+C' or 'Ctrl+Insert'</ShortCut>
          <Description>Copy</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Declaration">
        <Item>
          <IdeaActionID>GotoDeclaration</IdeaActionID>
          <ShortCut>'Ctrl+B' or 'Ctrl+Button1 Click'</ShortCut>
          <Description>Declaration</Description>
          <Count>3</Count>
        </Item>
      </Statistic>
    </option>
  </component>
</application>